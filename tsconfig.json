{
  "compilerOptions": {
    "declaration": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "preserveConstEnums": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "ESNext"
  },
  "exclude": [
    "node_modules"
  ],
  "types": [
    "node",
    "aws-sdk",
  ],
}
